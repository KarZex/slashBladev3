    "import numpy as np\n",
    "R = 24\n",
    "R2 = 12\n",
    "n = 9\n",
    "d = 3 * np.pi / 2\n",
    "a = d / (2*n)\n",
    "for i in range(-n+1,n):\n",
    "    x = np.sin(a*i) * R\n",
    "    z = (-np.cos(a*i) * R)\n",
    "    x2 = np.sin(a*i) * R2\n",
    "    z2 = (-np.cos(a*i) * R2)\n",
    "    print(\"x:{0},z:{1}  x2:{2},z2:{3}\".format(x,z,x2,z2))"
   ]
  }
