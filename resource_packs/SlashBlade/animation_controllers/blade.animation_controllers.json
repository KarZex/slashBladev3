{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.blade" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"transitions" : [
						{
							"gpre" : "query.is_using_item && !query.is_sneaking && query.is_on_ground"
						},
						{
							"pre" : "query.is_using_item && !query.is_sneaking && !query.is_on_ground"
						},
						{
							"melee" : "query.is_delayed_attacking && !query.is_using_item"
						},
						{
							"fall" : "query.is_sneaking && !query.is_jumping && query.is_using_item && !query.is_on_ground"
						},
						{
							"is_using_item_pre" : "query.is_sneaking && query.is_using_item && ( query.is_on_ground && !query.is_jumping ) && query.is_moving"
						}
					]
				},
				"is_using_item_pre":{
					"transitions": [
						{
							"ground_move": "!query.is_using_item && query.state_time < 0.5"
						},
						{
							"usesa": "!query.is_using_item && query.state_time >= 0.5"
						}
					]
				},
				"ground_move":{
					"animations": [
						"use"
					],
					"transitions": [
						{
							"ground_move1": "query.state_time > 0.15"
						}
					]
				},
				"ground_move1":{
					"animations": [
						"use2"
					],
					"transitions": [
						{
							"ground_move2": "query.state_time > 0.15"
						}
					]
				},
				"ground_move2":{
					"animations": [
						"use3"
					],
					"transitions": [
						{
							"default": "query.state_time > 0.15"
						}
					]
				},
				"gpre" : {
					"transitions" : [
						{
							"gattack" : "!query.is_using_item && ((!query.is_sneaking || query.is_on_ground) && query.state_time <= 0.5)"
						},
						{
							"usesa" : "!query.is_using_item && ((query.is_sneaking && !query.is_on_ground) || query.state_time > 0.5)"
						},
						{
							"default" : "!query.is_on_ground"
						}
					]
				},
				"gattack" : {
					"animations" : [ "gattack" ],
					"transitions" : [
						{
							"gpre2" : "query.is_using_item && query.state_time > 0.15"
						},
						{
							"default" : "query.state_time > 1.0 || !query.is_on_ground"
						}
					]
				},
				"gpre2" : {
					"transitions" : [
						{
							"gattack2" : "!query.is_using_item && ((!query.is_sneaking || query.is_on_ground) && query.state_time <= 0.5)"
						},
						{
							"default" : "!query.is_on_ground"
						}
					]
				},
				"gattack2" : {
					"animations" : [ "gattack2" ],
					"transitions" : [
						{
							"gpre3" : "query.is_using_item && query.state_time > 0.15"
						},
						{
							"default" : "query.state_time > 1.0 || !query.is_on_ground"
						}
					]
				},
				"gpre3" : {
					"transitions" : [
						{
							"gattack3" : "!query.is_using_item && ((!query.is_sneaking || query.is_on_ground) && query.state_time <= 0.5)"
						},
						{
							"default" : "!query.is_on_ground"
						}
					]
				},
				"gattack3" : {
					"animations" : [ "use" ],
					"transitions" : [
						{
							"gpre" : "query.is_using_item && !query.is_sneaking && query.is_on_ground"
						},
						{
							"pre" : "query.is_using_item && !query.is_sneaking && !query.is_on_ground"
						},
						{
							"melee" : "query.is_delayed_attacking && !query.is_using_item"
						},
						{
							"fall" : "query.is_sneaking && query.is_jumping && query.is_using_item && !query.is_on_ground"
						},
						{
							"nattack" : "query.is_sneaking && query.is_using_item && ( query.is_on_ground || query.is_jumping )"
						}
					]
				},
				"pre" : {
					"transitions" : [
						{
							"attack" : "!query.is_using_item && ((!query.is_sneaking || query.is_on_ground) && query.state_time <= 0.5)"
						},
						{
							"usesa" : "!query.is_using_item && ((query.is_sneaking && !query.is_on_ground) || query.state_time > 0.5)"
						}
					]
				},
				"usesa" : {
					"animations" : [ "gattack" ],
					"transitions" : [
						{
							"default" : "query.state_time > 0.5"
						}
					]
				},

				"nattack" : {
					"animations" : [ "use" ],
					"transitions" : [
						{
							"default" : "query.state_time > 0.5"
						}
					]
				},


				"attack" : {
					"animations" : [ "use" ],
					"transitions" : [
						{
							"pre2" : "query.is_using_item && query.state_time > 0.15"
						},
						{
							"default" : "query.state_time > 1.0 || query.is_on_ground"
						}
					]
				},
				"pre2" : {
					"transitions" : [
						{
							"attack2" : "!query.is_using_item"
						}
					]
				},
				"attack2" : {
					"animations" : [ "use2" ],
					"transitions" : [
						{
							"pre3" : "query.is_using_item && query.state_time > 0.15"
						},
						{
							"default" : "query.state_time > 1.0 || query.is_on_ground"
						}
					]
				},
				"pre3" : {
					"transitions" : [
						{
							"attack3" : "!query.is_using_item"
						}
					]
				},
				"attack3" : {
					"animations" : [ "use3" ],
					"transitions" : [
						{
							"pre4" : "query.is_using_item && query.state_time > 0.15"
						},
						{
							"default" : "query.state_time > 1.0 || query.is_on_ground"
						}
					]
				},
				"pre4" : {
					"transitions" : [
						{
							"attack4" : "!query.is_using_item"
						}
					]
				},
				"attack4" : {
					"animations" : [ "use4" ],
					"transitions" : [
						{
							"pre" : "query.is_using_item && query.state_time > 0.15"
						},
						{
							"default" : "query.state_time > 1.0"
						}
					]
				},
				"pre5" : {
					"transitions" : [
						{
							"attack5" : "!query.is_using_item"
						}
					]
				},
				"attack5" : {
					"animations" : [ "use" ],
					"transitions" : [
						{
							"pre" : "query.is_using_item && query.state_time > 0.15"
						},
						{
							"default" : "query.state_time > 1.0 || query.is_on_ground"
						}
					]
				},
				"melee" : {
					"animations" : [ "atk" ],
					"transitions" : [
						{
							"pre" : "query.is_using_item"
						},
						{
							"default" : "query.state_time > 0.5"
						}
					]
				},
				"fall" : {
					"transitions" : [
						{
							"attack4" : "!query.is_using_item"
						}
					]
				}

			}
		}
	}
}
