{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.bladeshadow": {
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "material.default"
        }
      ],
      "textures": [
        "Array.item_texture[query.skin_id]"
      ],
      "arrays": {
        "textures": {
          "Array.item_texture": [
            "texture.white",
            "texture.orange",
            "texture.magenta",
            "texture.light_blue",
            "texture.yellow",
            "texture.lime",
            "texture.pink",
            "texture.gray",
            "texture.light_gray",
            "texture.cyan",
            "texture.purple",
            "texture.blue",
            "texture.brown",
            "texture.green",
            "texture.red",
            "texture.black"
          ]
        }
      },
			"uv_anim": {
				"offset": [
					0.0,
					"math.mod(math.floor(query.life_time*10),10) / 10"
				],
				"scale": [
					1.0,
					"1 / 10"
				]
			}
    }
  }
}